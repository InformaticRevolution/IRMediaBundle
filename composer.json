{
    "name": "informaticrevolution/media-bundle",
    "type": "symfony-bundle",
    "description": "Symfony IRMediaBundle",
    "keywords": ["media"],
    "license": "MIT",
    "authors": [
        {
            "name": "Julien Kirsch",
            "email": "informatic.revolution@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "2.3.*",
        "knplabs/gaufrette": "0.2.x-dev",
        "imagine/Imagine": "*@stable",
        "kriswallsmith/buzz": "0.*"        
    },
    "require-dev": {
	"twig/twig": "*",
        "doctrine/doctrine-bundle": "1.2.*",
	"symfony/yaml": "2.3.*",
        "symfony/validator": "2.3.*",
        "amazonwebservices/aws-sdk-for-php": "1.5.*"
    },   
    "suggest": {
        "amazonwebservices/aws-sdk-for-php": "1.5.*"
    },    
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "IR\\Bundle\\MediaBundle": "" }
    },
    "target-dir": "IR/Bundle/MediaBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}